{
  "version": "v1.8.6",
  "imports": {
    "imagescript": "https://deno.land/x/imagescript@1.3.0/mod.ts",
    "@std/encoding": "jsr:@std/encoding@^1.0.5",
    "@std/fs": "jsr:@std/fs@^1.0.21",
    "@std/path": "jsr:@std/path@^1.1.4",
    "s3-lite-client": "jsr:@bradenmacdonald/s3-lite-client@^0.7.1",
    "@std/assert": "jsr:@std/assert@^1.0.0",
    "@gradio/client": "npm:@gradio/client@^1.0.0"
  },
  "compilerOptions": {
    "lib": ["deno.window", "deno.ns"],
    "strict": true
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 100,
    "indentWidth": 2,
    "singleQuote": false
  },
  "lint": {
    "rules": {
      "tags": ["recommended"]
    }
  },
  "tasks": {
    "dev": "deno run --allow-net --allow-env --allow-read --allow-write --allow-run=docker,docker-compose --watch src/main.ts",
    "start": "deno run --allow-net --allow-env --allow-read --allow-write --allow-run=docker,docker-compose src/main.ts"
  }
}
